
<head>
  <title>Table V02</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">


   <link rel="stylesheet" type="text/less" href="/styles/pages/dashboard/subject.less">
<!--===============================================================================================-->
</head>
<body>

<div id="subject">
<div class="limiter">
  <div class="container-table100">
    <div class="wrap-table100">
<div class="table100 ver3 m-b-110">
  <div class="table100-head">
    <table>
      <thead>
      <tr class="row100 head">
        <th class="cell100 column1">Subject name</th>
        <th class="cell100 column2">Lecturer ID</th>
        <th class="cell100 column3">Subject ID</th>
        <th class="cell100 column4">View More</th>
      </tr>
      </thead>
    </table>
  </div>
  <div class="table100-body js-pscroll">
    <table>

      <% for (var i = 0; i < subject.length; i++) { %>
      <tbody>
      <tr class="row100 body">
        <td class="cell100 column1"><%= subject[i].subjectName %></td>
        <td class="cell100 column2"><%= subject[i].lecturer %></td>
        <td class="cell100 column3"><%= subject[i].id %></td>
       <!-- <td class="cell100 column4"><button type="button"  onclick="location.href='/lesson?id=<%= subject[i].id %>'">View More</button></td> -->
        <td class="cell100 column4"><a href="/lesson?id=<%= subject[i].id %>" class="button4" style="background-color:#f14e4e">View More</a></td>
      </tr>
      </tbody>
        <% } %>
    </table>
  </div>
</div>
    </div>
  </div>
</div>
</div>
</body>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
<script>
  $('.js-pscroll').each(function(){
    var ps = new PerfectScrollbar(this);

    $(window).on('resize', function(){
      ps.update();
    })
  });


</script>
<!--===============================================================================================-->
